<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="snn_plat"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
snn_plat
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="snn_plat">
        <sdx:configuration sdx:name="snn_plat"
                           sdx:displayName="snn_plat"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>snn_plat</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="snn_plat/boot/snn_plat.bif"
                            sdx:readme="snn_plat/boot/generic.readme"
                            sdx:qemuArguments="snn_plat/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="snn_plat/boot/snn_plat.bif"
                            sdx:readme="snn_plat/boot/generic.readme"
                            sdx:qemuArguments="snn_plat/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="snn_plat/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="snn_plat/standalone_domain/bspinclude/include"
                        sdx:bspConfig="snn_plat/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
